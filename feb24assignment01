input:

l= []
e = []
o = []
n = int(input("No.of elements :- "))
for i in range(1,n+1):
    value = int(input("" ))
    l.append(value)
for j in range(n):
    if(l[j] % 2 == 0):
        e.append(l[j])
    else:
        o.append(l[j])
print(l)
print("Elements in Even List is : ", e)
print("Elements in Odd List is : ", o)
